{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Employe</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Salairenet</th>
                <td>{{ entity.salairenet }}</td>
            </tr>
            <tr>
                <th>Dateembauche</th>
                <td>{{ entity.dateEmbauche|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Nbrejourconge</th>
                <td>{{ entity.nbreJourConge }}</td>
            </tr>
            <tr>
                <th>Matricule</th>
                <td>{{ entity.matricule }}</td>
            </tr>
            <tr>
                <th>Fonction</th>
                <td>{{ entity.fonction }}</td>
            </tr>
            <tr>
                <th>Datefincontrat</th>
                <td>{{ entity.datefinContrat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datedebutcontrat</th>
                <td>{{ entity.datedebutContrat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('employe') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('employe_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
